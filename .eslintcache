.push(\"/\");\n    }\n\n    let result = null;\n    setIsMovie(pathname.includes(\"/movie/\"));\n\n    try {\n      if (isMovie) {\n        ({ data: result } = await movieApi.movieDetail(parsedId));\n      } else {\n        ({ data: result } = await tvApi.showDetail(parsedId));\n      }\n      setResult(result);\n    } catch {\n      setError(\"데이터를 찾을 수 없습니다.\");\n    } finally {\n      setLoading(false);\n    }\n  };\n\n  useEffect(() => {\n    getDetailDataFromApi();\n  }, []);\n  return <DetailViewer result={result} error={error} loading={loading} />;\n};\n\nexport default withRouter(DetailContainer);\n","/Users/seoljaehyeok/hyeokflix/src/containers/Search/SearchContainer.js",[],["129","130"],"/Users/seoljaehyeok/hyeokflix/src/components/common/Section.js",[],"/Users/seoljaehyeok/hyeokflix/src/containers/Tv/index.js",[],"/Users/seoljaehyeok/hyeokflix/src/components/common/Poster.js",[],"/Users/seoljaehyeok/hyeokflix/src/components/common/Message.js",[],"/Users/seoljaehyeok/hyeokflix/src/containers/Search/index.js",[],"/Users/seoljaehyeok/hyeokflix/src/components/common/Company.js",[],"/Users/seoljaehyeok/hyeokflix/src/components/common/Casting.js",[],"/Users/seoljaehyeok/hyeokflix/src/components/common/Video.js",[],"/Users/seoljaehyeok/hyeokflix/src/lib/GlobalStyles.js",[],"/Users/seoljaehyeok/hyeokflix/src/components/common/Country.js",[],{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","severity":1,"message":"136","line":16,"column":1,"nodeType":"137","endLine":37,"endColumn":3},{"ruleId":"131","replacedBy":"138"},{"ruleId":"133","replacedBy":"139"},{"ruleId":"135","severity":1,"message":"136","line":1,"column":1,"nodeType":"137","endLine":1,"endColumn":33},{"ruleId":"131","replacedBy":"140"},{"ruleId":"133","replacedBy":"141"},{"ruleId":"131","replacedBy":"142"},{"ruleId":"133","replacedBy":"143"},{"ruleId":"144","severity":1,"message":"145","line":41,"column":6,"nodeType":"146","endLine":41,"endColumn":8,"suggestions":"